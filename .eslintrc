{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "spaced-comment": "off",
    "linebreak-style": [
      "error",
      "windows"
    ],
    "operator-linebreak": "off",
    "max-len": "off"
  },
  "extends": [
    "eslint:recommended",
    "google"
  ]
}